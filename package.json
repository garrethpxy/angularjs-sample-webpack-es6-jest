{
  "name": "sample-ng15-webpack-babel-karma",
  "version": "1.0.0",
  "description": "Sample repository showing integration of Angular 1.5, webpack, babel, and karma tests",
  "main": "index.js",
  "author": "Kamran Ayub <kamran.ayub@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "1.5.8",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "angular-mocks": "1.5.8",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-preset-env": "1.6.0",
    "concurrently": "3.5.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "21.2.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "raw-loader": "0.5.1",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.4"
  },
  "scripts": {
    "start": "webpack-dev-server --config=webpack.config.js",
    "test": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setup.js",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^(?!.*\\.(js|jsx|json)$)": "<rootDir>/jest/fileTransform.js"
    }
  }
}
